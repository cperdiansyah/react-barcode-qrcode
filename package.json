{
  "name": "react-barcode-qrcode",
  "private": false,
  "version": "0.1.0-alpha.0",
  "description": "A react library for generating qrcode and barcode.",
  "author": "cperdiansyah <chandraperdiansyah@gmail.com>",
  "license": "MIT",
  "type": "module",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "keywords": [
    "react",
    "reactjs",
    "barcode",
    "qrcode",
    "react-barcode",
    "react-qrcode",
    "react-barcode-qrcode",
    "react-qrcode-barcode"
  ],
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "lint": "eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "preview": "vite preview"
  },
  "dependencies": {
    "jsbarcode": "^3.11.5",
    "qrcode.react": "^3.1.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0"
  },
  "devDependencies": {
    "@types/node": "^20.5.6",
    "@types/react": "^18.2.15",
    "@types/react-dom": "^18.2.7",
    "@typescript-eslint/eslint-plugin": "^6.0.0",
    "@typescript-eslint/parser": "^6.0.0",
    "@vitejs/plugin-react": "^4.0.3",
    "eslint": "^8.48.0",
    "eslint-plugin-react": "^7.33.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-refresh": "^0.4.3",
    "typescript": "^5.0.2",
    "vite": "^4.4.5",
    "vite-plugin-dts": "^3.5.2"
  },
  "files": [
    "dist"
  ],
  "bundlesize": [
    {
      "path": "./dist/*.js",
      "maxSize": "21 kB"
    },
    {
      "path": "./dist/**/*.js",
      "maxSize": "21 kB"
    },
    {
      "path": "./dist/*.ts",
      "maxSize": "750 B"
    },
    {
      "path": "./dist/**/*.ts",
      "maxSize": "770 B"
    }
  ]
}